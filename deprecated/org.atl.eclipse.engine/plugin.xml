<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.0"?>
<plugin
   id="org.atl.eclipse.engine"
   name="ATL Engine"
   version="1.0.7"
   provider-name="ATLAS"
   class="org.atl.eclipse.engine.EnginePlugin">

   <runtime>
      <library name="org.atl.eclipse.engine.jar">
         <export name="*"/>
      </library>
      <library name="lib/atl.jar">
         <export name="*"/>
      </library>
      <library name="lib/emf4atl.jar">
         <export name="*"/>
      </library>
      <library name="lib/ebnfinjector.jar">
         <export name="*"/>
      </library>
      <library name="lib/antlr.jar">
         <export name="*"/>
      </library>
      <library name="lib/ATL-importer.jar"/>
      <library name="lib/mdr4atl.jar"/>
      <library name="lib/jmi.jar"/>
      <library name="lib/jmiutils.jar"/>
      <library name="lib/mdrapi.jar"/>
      <library name="lib/mof.jar"/>
      <library name="lib/nbmdr.jar"/>
      <library name="lib/openide-util.jar"/>
   </runtime>
   <requires>
      <import plugin="org.eclipse.ui"/>
      <import plugin="org.eclipse.core.runtime"/>
      <import plugin="org.eclipse.emf.ecore"/>
      <import plugin="org.eclipse.core.resources"/>
      <import plugin="org.eclipse.emf.ecore.xmi"/>
   </requires>


</plugin>
