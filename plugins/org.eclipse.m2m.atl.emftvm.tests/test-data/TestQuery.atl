query TestQuery = EMFTVM!Operation.allInstances()->select(r|r.test()).debug('TestQuery');

uses TestLib;

helper context EMFTVM!Operation def : test() : Boolean =
	(super.test() and not self.static).debug('TestQuery::Operation.test()');