module MatchedRuleWithFilterAndAction;
create OUT : EMFTVM, PBS : Problem from IN : ATL;

lazy rule InvokeIncluding {
 from
  s : ATL!OclExpression (true)
 to
  invoke : EMFTVM!Invoke (
   opname <- 'including',
   argcount <- 1
  )
 do {
  invoke;
 }
}